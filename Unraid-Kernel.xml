<?xml version="1.0"?>
<Container version="2">
  <Name>Unraid-Kernel</Name>
  <Repository>ghcr.io/ich777/unraid_kernel:latest</Repository>
  <Registry>https://github.com/ich777/unraid_kernel/pkgs/container/unraid_kernel</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://unraid.net/</Project>
  <Overview/>
  <Category>Tools:System</Category>
  <WebUI/>
  <TemplateURL>False</TemplateURL>
  <Icon>https://raw.githubusercontent.com/ich777/docker-templates/master/ich777/images/unraid.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled/>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Host Path 1" Target="/usr/src" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/cache/appdata/unraid-kernel</Config>
  <Config Name="Host Port 1" Target="8022" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">8020</Config>
  <Config Name="Host Key 1" Target="ENABLE_SSH" Default="true|false" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">true</Config>
  <Config Name="Host Key 6" Target="ROOT_PWD" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="true">secret</Config>
  <Config Name="Host Key 2" Target="DL_ON_START" Default="true|false" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">true</Config>
  <Config Name="Host Key 3" Target="EXTRACT" Default="true|false" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">true</Config>
  <Config Name="Host Key 4" Target="CPU_THREADS" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">all</Config>
  <Config Name="Host Key 5" Target="CLEANUP" Default="false|true" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">false</Config>
</Container>
